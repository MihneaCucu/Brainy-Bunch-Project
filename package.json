{
  "name": "brainy-bunch",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "npx nodemon index.js",
    "lint": "eslint .",
    "test": "jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "db:push": "sequelize-cli db:migrate",
    "db:migrate:make": "sequelize-mig migration:make",
    "db:migrate:new": "sequelize-cli migration:generate --name",
    "db:seed": "sequelize-cli db:seed:all",
    "db:drop": "sequelize-cli db:drop",
    "db:create": "sequelize-cli db:create",
    "db:reset": "sequelize-cli db:migrate:undo:all && sequelize-cli db:migrate && sequelize-cli db:seed:all"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "graphql-http": "^1.22.4",
    "jsonwebtoken": "^9.0.2",
    "process": "^0.11.10",
    "sequelize": "^6.37.5",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@faker-js/faker": "^10.1.0",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "jest": "^30.2.0",
    "sequelize-cli": "^6.6.3",
    "sequelize-mig": "^3.1.3"
  }
}
